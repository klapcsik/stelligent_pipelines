{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "Store CFN Outputs to consume in downstream CFN stacks",
  "Parameters": {
    "UUID": {
      "Type": "String",
      "Description": "Unique identifier to uniquely name Resources",
      "Default": "EMPTY"
    },
    "MasterStackName": {
      "Type": "String",
      "Description": "Name of the master.json CFN stack",
      "Default": "EMPTY"
    },
    "DromedaryS3Bucket": {
      "Type": "String",
      "Description": "Name of S3 bucket used to store Jenkins config",
      "Default": "EMPTY"
    },
    "DemoResultsBucket": {
      "Type": "String",
      "Description": "Name of the S3 bucket used to store test results",
      "Default": "EMPTY"
    },
    "Branch": {
      "Type": "String",
      "Description": "Name of Dromedary Github branch",
      "Default": "EMPTY"
    },
    "KeyName": {
      "Type": "String",
      "Description": "EC2 KeyPair",
      "Default": "EMPTY"
    },
    "MyBuildProvider": {
      "Type": "String",
      "Description": "Jenkins Build Provider Name",
      "Default": "EMPTY"
    },
    "JobConfigsTarball": {
      "Type": "String",
      "Description": "S3 key for DromedaryS3Bucket",
      "Default": "EMPTY"
    },
    "Hostname": {
      "Type": "String",
      "Description": "subdomain name",
      "Default": "EMPTY"
    },
    "Domain": {
      "Type": "String",
      "Description": "Route53 Hosted Zone name for prod IP (include trailing .)",
      "Default": "oneclickdeployment.com."
    },
    "ProdHostedZone": {
      "Type": "String",
      "Description": "Route53 Hosted Zone (e.g. PRODHOST.HOSTED.ZONE)",
      "AllowedPattern": "^.*?\\..*?\\..*$"
    },
    "VPCStackName": {
      "Type": "String",
      "Description": "VPCStackName",
      "Default": "EMPTY"
    },
    "IAMStackName": {
      "Type": "String",
      "Description": "IAMStackName",
      "Default": "EMPTY"
    },
    "DDBStackName": {
      "Type": "String",
      "Description": "DDBStackName",
      "Default": "EMPTY"
    },
    "ConfigStackName": {
      "Type": "String",
      "Description": "ConfigStackName",
      "Default": "EMPTY"
    },
    "LambdaConfigStackName": {
      "Type": "String",
      "Description": "LambdaConfigStackName",
      "Default": "EMPTY"
    },
    "ENIStackName": {
      "Type": "String",
      "Description": "ENIStackName",
      "Default": "EMPTY"
    },
    "DromedaryAppURL": {
      "Type": "String",
      "Description": "The URL users use to launch the Dromedary application",
      "Default": "EMPTY"
    },
    "ZapStackName": {
      "Type": "String",
      "Description": "ZapStackName",
      "Default": "EMPTY"
    }
  },
  "Resources": {
    "MyQueue": {
      "Type": "AWS::SQS::Queue",
      "Properties": {
        "QueueName": {
          "Fn::Join": [
            "",
            [
              "PipelineStoreQueue-",
              {
                "Ref": "UUID"
              }
            ]
          ]
        }
      }
    }
  },
  "Outputs": {
    "StackName": {
      "Value": {
        "Ref": "AWS::StackName"
      }
    },
    "UUID": {
      "Value": {
        "Ref": "UUID"
      }
    },
    "MasterStackName": {
      "Value": {
        "Ref": "MasterStackName"
      },
      "Description": "Name of the Master CFN Stack"
    },
    "DromedaryS3Bucket": {
      "Value": {
        "Ref": "DromedaryS3Bucket"
      },
      "Description": "Name of S3 bucket used to store Jenkins config"
    },
    "DemoResultsBucket": {
      "Value": {
        "Ref": "DemoResultsBucket"
      },
      "Description": "Name of the S3 bucket used to store test results"
    },
    "Branch": {
      "Value": {
        "Ref": "Branch"
      },
      "Description": "TBD"
    },
    "KeyName": {
      "Value": {
        "Ref": "KeyName"
      },
      "Description": "TBD"
    },
    "MyBuildProvider": {
      "Value": {
        "Ref": "MyBuildProvider"
      },
      "Description": "TBD"
    },
    "JobConfigsTarball": {
      "Value": {
        "Ref": "JobConfigsTarball"
      },
      "Description": "S3 key for DromedaryS3Bucket"
    },
    "Hostname": {
      "Value": {
        "Ref": "Hostname"
      },
      "Description": "subdomain name"
    },
    "Domain": {
      "Value": {
        "Ref": "Domain"
      },
      "Description": "Route53 Hosted Zone name for prod IP (include trailing .)"
    },
    "ProdHostedZone": {
      "Value": {
        "Ref": "ProdHostedZone"
      },
      "Description": "Route53 Hosted Zone (e.g. PRODHOST.HOSTED.ZONE)"
    },
    "VPCStackName": {
      "Value": {
        "Ref": "VPCStackName"
      },
      "Description": "TBD"
    },
    "IAMStackName": {
      "Value": {
        "Ref": "IAMStackName"
      },
      "Description": "TBD"
    },
    "DDBStackName": {
      "Value": {
        "Ref": "DDBStackName"
      },
      "Description": "TBD"
    },
    "ENIStackName": {
      "Value": {
        "Ref": "ENIStackName"
      },
      "Description": "TBD"
    },
    "ConfigStackName": {
      "Value": {
        "Ref": "ConfigStackName"
      },
      "Description": "TBD"
    },
    "LambdaConfigStackName": {
      "Value": {
        "Ref": "LambdaConfigStackName"
      },
      "Description": "TBD"
    },
    "DromedaryAppURL": {
      "Value": {
        "Ref": "DromedaryAppURL"
      },
      "Description": "The URL users use to launch the Dromedary application"
    },
    "ZapStackName": {
      "Value": {
        "Ref": "ZapStackName"
      },
      "Description": "Zap Stack to query"
    }
  }
}
